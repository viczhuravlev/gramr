{
  "name": "gramr",
  "version": "0.0.1",
  "description": "React a UI component library",
  "main": "lib/index.js",
  "types": "lib/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viczhuravlev/gramr.git"
  },
  "homepage": "https://github.com/viczhuravlev/gramr#readme",
  "bugs": {
    "url": "https://github.com/viczhuravlev/gramr/issues"
  },
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "emotion",
    "ui"
  ],
  "author": "Victor Zhuravlev <vzhuravlev15@gmail.com>",
  "license": "GPL version 3",
  "scripts": {
    "build": "npm run build:source && npm run build:docs",
    "build:docs": "docz build",
    "build:docs:deploy": "npm run build:docs && gh-pages -d .docz/dist",
    "build:source": "npm run build:source:lib && npm run build:source:types",
    "build:source:lib": "webpack --mode production",
    "build:source:types": "tsc --emitDeclarationOnly",
    "lint": "npm run lint:code && npm run lint:types",
    "lint:code": "npm run lint:code:styles && npm run lint:code:source",
    "lint:code:styles": "stylelint './src/**/*.{ts,tsx}'",
    "lint:code:source": "eslint './src/**/*.{ts,tsx}'",
    "lint:types": "npm run lint:types:check && npm run lint:types:coverage",
    "lint:types:check": "tsc --noEmit",
    "lint:types:coverage": "type-coverage --ignore-catch --at-least 99 --project src",
    "dev": "docz dev",
    "prettier": "prettier --write 'src/**/*.{ts,tsx,mdx}'",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@emotion/core": "10.0.7",
    "@emotion/styled": "10.0.7",
    "react": "16.8.3",
    "react-dom": "16.8.3",
    "typescript": "3.9.5"
  },
  "peerDependencies": {
    "react": ">=16.6.0 < 17.0.0",
    "react-dom": ">=16.6.0 < 17.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.10.3",
    "@babel/preset-env": "7.10.3",
    "@babel/preset-react": "7.10.1",
    "@babel/preset-typescript": "7.10.1",
    "@commitlint/cli": "9.0.1",
    "@commitlint/config-conventional": "9.0.1",
    "@svgr/webpack": "5.4.0",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "26.0.3",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.8.2",
    "@typescript-eslint/eslint-plugin": "3.4.0",
    "@typescript-eslint/parser": "3.4.0",
    "babel-jest": "26.1.0",
    "babel-loader": "8.1.0",
    "babel-plugin-emotion": "10.0.33",
    "docz": "0.13.7",
    "docz-theme-default": "0.13.7",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "enzyme-to-json": "3.5.0",
    "eslint": "7.3.1",
    "eslint-config-airbnb-typescript": "8.0.2",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.21.2",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "4.0.4",
    "gh-pages": "2.0.1",
    "husky": "4.2.5",
    "jest": "26.1.0",
    "jest-svg-transformer": "1.0.0",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "source-map-loader": "1.0.0",
    "stylelint": "13.6.1",
    "stylelint-config-recess-order": "2.0.4",
    "stylelint-config-standard": "20.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-order": "4.1.0",
    "ts-jest": "26.1.1",
    "type-coverage": "2.8.2",
    "webpack": "4.43.0",
    "webpack-cli": "3.3.12",
    "webpack-node-externals": "1.7.2"
  }
}
